* p_lForceUpdate = .T. && Can be used to overwrite text in language.xml
*****

lcSafety = SET("Safety")
IF p_CurLanguage <> "language" AND .F.
     SELECT 0
     USE data\language SHARED
     DELETE ALL
     APPEND FROM DBF(p_CurLanguage)
     USE
ENDIF
CURSORTOXML(p_CurLanguage, "lcXML", 3, 32+2+4, 0, "1")
SET SAFETY OFF
STRTOFILE(STRCONV(lcXML,9), "Metadata\language.xml")
SET SAFETY &lcSafety
USE IN (p_CurLanguage)

SELECT (lnArea)

WAIT CLEAR

RETURN .T.
ENDFUNC

FUNCTION updatelanguage
LPARAMETERS tcProgram, tcLanguage, tcLabel, tcText, lOverwrite

DO CASE
    CASE NOT SEEK(PADR(tcLanguage,3)+PADR(tcProgram,8)+PADR(tcLabel,30),p_CurLanguage,"tag1")
        INSERT INTO (p_CurLanguage) (la_label, la_lang, la_prg, la_text) VALUES (tcLabel, tcLanguage, tcProgram, tcText)
    CASE lOverwrite OR p_lForceUpdate
        REPLACE la_text WITH tcText IN (p_CurLanguage)
    OTHERWISE
ENDCASE

RETURN .T.
ENDFUNC

LOCAL lnArea, lcXML, lcSafety
PRIVATE p_lForceUpdate, p_CurLanguage
lnArea = SELECT()
p_lForceUpdate = .F.

IF .T. && Set to .F. to update language.dbf
     p_CurLanguage = SYS(2015)
     WAIT WINDOW "Updating Language.xml..." NOWAIT
     lcXML = STRCONV(FILETOSTR("Metadata\language.xml"),11)
     XMLTOCURSOR(lcXML, p_CurLanguage)
     INDEX ON UPPER(la_lang+la_prg+la_label) TAG Tag1
     SET ORDER TO
ELSE
     USE data\language SHARED IN 0
     p_CurLanguage = "language"
ENDIF


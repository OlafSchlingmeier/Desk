	*</DefinedPropArrayMethod>

	BorderWidth = 0
	chtmlfile = 
	ddate = {}
	Height = 600
	lfirsttime = .T.
	Name = "cnthtml5"
	ohotelstat = .NULL.		&& Reference to chotelstatmydesk class object
	Width = 800

	ADD OBJECT 'oweb' AS olecontrol WITH ;
		Height = 600, ;
		Left = 0, ;
		Name = "oweb", ;

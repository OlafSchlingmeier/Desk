	*<DefinedPropArrayMethod>
		*m: customize
		*m: getactions
		*m: gethotstat
		*m: getnotes
		*m: getparams
		*m: getweather
		*m: go
		*m: navigateactions
		*m: navigatestickynote
		*m: onfirsttime
		*m: showstart
		*m: urlunescape

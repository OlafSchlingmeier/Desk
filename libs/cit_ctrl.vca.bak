			INDEX ON rt_roomtyp TAG TAG1
		ENDIF
		IF EMPTY(this.crtdcur)
			this.crtdcur = sqlcursor("SELECT rd_rdid, rd_roomtyp FROM rtypedef")
			INDEX ON rd_rdid TAG TAG1
		ENDIF
		
		this.cCursorName = SYS(2015)
		
		this.InitSqlProperties()
		
		DODEFAULT()
		
		this.RequeryCursor()
		
		RETURN .T.
	ENDPROC


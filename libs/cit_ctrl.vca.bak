				"WHERE uf_userid = " + sqlcnv(PADR(g_userid,10),.T.) + ;
				" AND uf_menu = " + sqlcnv(PADR(tcMenu,20),.T.) + " AND uf_menukey = " + sqlcnv(PADR(tcMenuKey,20),.T.))
		
		IF RECCOUNT(l_cCurUF)>0

	caltchar = !
	ccursor = 
	coldval = 
	ColumnCount = 3
	ColumnLines = .F.
	ColumnWidths = "70,120,100"
	corder = rc_ratecod
	crowsource = rc_ratecod,rc_lang,rc_rttype
	csqlselect = 
	csqlunion = 
	cudrchar = *
	cwhere = 
	DisabledForeColor = 0,0,0
	Format = "K"
	InputMask = (REPLICATE("!",10))

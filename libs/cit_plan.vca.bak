	PROCEDURE releasetextline
		LPARAMETERS tnIndex
		LOCAL loResTextLine
		
		IF this.ActiveHead > tnIndex
			loResTextLine = this.Pool[tnIndex]
			this.Pool[tnIndex] = this.Pool[this.ActiveHead]
			this.Pool[tnIndex].ArrPosition = tnIndex

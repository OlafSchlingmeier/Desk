			ENDIF
			this.RoomsInf[i, 6].Top = this.MainBorder.Top + i * this.ColumnHeight + 1
			this.RoomsInf[i, 6].Height = 0
			this.RoomsInf[i, 6].Left = this.MainBorder.Left + 1

		IF lnUnsGrid > 0
			this.grdRpUnasgn.Top = 6 * ScaleHeight
			this.grdRpUnasgn.Height = (500 + lnJumpButtonsHeight + lnAvlGrid) * ScaleHeight
			this.grdRpUnasgn.Left = 4 * ScaleWidth

		*			this.ActiveHead = this.ActiveHead - 1
				else
					this.Pool[i].BkLeft = this.Pool[i].BkLeft + m.ByPixels
					i = i + 1
				endif
			enddo
		
			this.DisplayLine(2, -m.ByStep)
		endif
		

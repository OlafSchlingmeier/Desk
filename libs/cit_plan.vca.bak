		*SFPX = MCol(this.Name) / MCol(this.Name, 3)
		*SFPY = MRow(this.Name) / MRow(this.Name, 3)
		
		this.GFoxTop = this.GreenBk.Top - Int(this.ColumnHeight / 2)
		this.GFoxLeft = this.GreenBk.Left - Int(this.ColumnWidth / 2)
		

	PROCEDURE Init
		*mngcwvrrt.scx  frmmngcwrrt.init
		LPARAMETERS tnMode, tcVirtRoomtype, tcAdditionalMode
		LOCAL lcRoomTypes, lcRTypeNames, lcMessage, curRtype, curChannel
		*SET STEP ON
		this.nmode = tnMode
		IF VARTYPE(tcAdditionalMode)='C'
			this.cmode = tcAdditionalMode
		ENDIF
		this.NewObject("oCitwebRates","CitwebRates","proc_yield.vcx")
		this.oCitwebRates.dSysDate = SysDate()
		this.oCitwebRates.cLangnum = g_Langnum
		this.oCitwebRates.cPathDesk = gcDatadir
		this.oCitwebRates.cPathCitweb = _screen.oGlobal.oParam2.pa_ciwebdr
		this.oCitwebRates.OpenAliases()
		this.oCitwebRates.OnInit(this.nmode, tcVirtRoomtype)
		
		IF USED(this.oCitwebRates.cvirtrooms)
			SELECT *, CAST('' AS C(254)) AS c_fortypes, CAST('' AS C(254)) AS c_fortname, CAST('' As C(1)) as c_channel;
				FROM (this.oCitwebRates.cvirtrooms) ORDER BY vr_rtname INTO CURSOR curVirtRooms READWRITE
		ELSE
			this.lNoCitwebConnection = .T.
			CREATE CURSOR curVirtRooms (vr_vroom C(3), vr_rtname C(10), c_fortypes C(254), c_fortname C(254), c_Channel c(1))	&& Fake data
		ENDIF
		
		IF NOT EMPTY(tcVirtRoomtype) AND NOT DLocate("curVirtRooms", "vr_vroom = " + SqlCnv(tcVirtRoomtype))
			INSERT INTO curVirtRooms (vr_vroom) VALUES (tcVirtRoomtype)
		ENDIF
		
		IF USED(this.oCitwebRates.cvirtrconn)
			SELECT curVirtRooms
			SCAN
				STORE "" TO lcRoomTypes, lcRTypeNames
				SELECT (this.oCitwebRates.cvirtrconn)
				SCAN FOR vc_vroom = curVirtRooms.vr_vroom
					lcRoomTypes = lcRoomTypes + IIF(EMPTY(lcRoomTypes), "", ",") + vc_room
					lcRTypeNames = lcRTypeNames + IIF(EMPTY(lcRTypeNames), "", ",") + Get_rt_roomtyp(vc_room)
				ENDSCAN
				SELECT curVirtRooms
				REPLACE c_fortypes WITH lcRoomTypes, c_fortname WITH lcRTypeNames, c_channel WITH "C"
			ENDSCAN
		ENDIF
		*
		* Hier werden die Zimmertypen aus dem Hotelsystem in die curVirtrooms überspielt 
		* os geändert 01.03.2021
		IF this.nmode = NEW_MODE THEN  && nur bei Neuanlage muss der Zimmertyp vom Hotel übermittelt werden
			* da dann erst durch festlegen des Channel-Kanals feststeht ob Channel oder Web/Hotel
			* Im Edit-Modus ist dies nicht erforderlich, da der Zimmertyp nicht mehr veränderbar ist
			curRtype = SYS(2015)  
			SELECT rd_roomtyp, rd_lang3, rd_rdid, rd_group, rt_buildng, rt_roomtyp ;
				FROM rtypedef JOIN roomtype ON rd_rdid = rt_rdid  WHERE rd_group = 1 INTO CURSOR curRtype
			SELECT curRtype
			GO top
			SCAN
				* vr_room wird "" gesetzt da das Feld in CitWeb 3 stellig ist im Hotelprogramm 4 stellig
				* Beim Einsetzen des Filter wird dann empty(vr_vroom) genommen, damit filter man dann die
				* Hotelkategorien. Zusätzlich wurde auch das Feld C_channel eingestellt. ist der Wert C = Channel
				* ist der Wert H = Hotel / Web
				INSERT INTO curVirtrooms (vr_Vroom, vr_rtname,vr_buildng, c_fortypes, c_fortname, C_Channel) VALUES ;
					("", curRtype.rd_roomtyp,curRtype.rt_buildng,curRtype.rt_roomtyp,ALLTRIM(curRtype.rd_roomtyp);
					 + " " +curRtype.rt_buildng, "H")
				*
			ENDSCAN
		ELSE
			*
			curChannel = SYS(2015)
			IF thisform.eq_channel.Value < 99
				thisform.eq_channel.rowsource = "SELECT code, id FROM cchannells where id < 99 into cursor &curChannel ORDER BY 1 ASC"
			ELSE
				thisform.eq_channel.rowsource = "SELECT code, id FROM cchannells where id > 98 into cursor &curChannel ORDER BY 1 ASC"
				thisform.optpricetype.optmax.Visible = .f.
				thisform.optpricetype.optmin.Caption = "Standardpreis - Preiscode"  
				
			endif
			*
		endif
		* os Änderung ende 
		*
		lcMessage = ""
		
		IF NOT USED(this.oCitwebRates.cvirtrooms)
			lcMessage = "virtrooms " + GetLangText("OPENFILE","TXT_CANTBEOPENEDSHARED")
		ENDIF
		IF NOT USED(this.oCitwebRates.cvirtrconn)
			lcMessage = lcMessage + CRLF + "virtrconn " + GetLangText("OPENFILE","TXT_CANTBEOPENEDSHARED")
		ENDIF
		IF NOT EMPTY(lcMessage)
			Alert(lcMessage)
		ENDIF
		IF thisform.eq_channel.value < 99
			thisform.lbleq_vroom.Caption = "CitWeb ZT"
		ELSE
			thisform.lbleq_vroom.Caption = "Kategorie"
		endif
		
		this.eq_ratecod.Init(.T.)
		DODEFAULT()
		
		this.eq_vroom.Enabled = (this.nmode <> EDIT_MODE)
		this.nPersons = this.oCitwebRates.oCwvrrt.eq_adults
		* Hier wird eingestellt welche Erw und Kinder Felder freigeschaltet werden
		
		this.grdCwRates.grdBaseSortColumn4.SetFocus()
		
		* Hier werden die neuen Felder Ew1 - Ew5 plus checkbox und kin1 - kin3 plus checkbox gesetzt
		*Thisform.tcbEw1.value = iif(thisform.eq_adults.Value>0 and this.nmode = EDIT_MODE,.t.,.f.)
		*
		IF this.nmode = NEW_MODE then
			*
			thisform.eq_vroom.Enabled = .t. 
			thisform.eq_vroom.Value = ""
		ENDIF
		*
		STORE iif(thisform.eq_adults.Value>0 and this.nmode = EDIT_MODE,.t.,.f.) TO Thisform.tcbew1.Value, Thisform.txtEw1.Enabled 

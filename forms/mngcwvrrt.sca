		IF lnSuccess = 0
			IF NOT this.oCitwebRates.oCwvrrt.eq_inactiv AND _screen.oGlobal.oParam2.pa_cwsync AND YesNo(GetLangText("MNGCWR","TXT_SEND_PRICES_NOW"))
				CCOnCitwebSync("all")
			ENDIF
			this.OnClose()
		ENDIF
		
		WAIT CLEAR
		
		RETURN .T.
	ENDPROC

	PROCEDURE preiseingabe		&& Schaltet die Felder txtEw1-txtEw5.enabled auf .t. wenn die Anzahl Personen >= Ew? ist und tcbew1-tcbew5 = .t. ist
		LPARAMETERS s1, s2
		LOCAL i, hf, hf2, x
		IF s1 > 0 Then
			IF s2 > 0 then
				x=s2
			ELSE
				s2 = s1
			ENDIF
		ELSE
			RETURN .t.
		ENDIF
		FOR i = s1 TO x
			hf  = "Thisform.txtEw" + TRANSFORM(i) + ".enabled"
			hf2 = "Thisform.tcbEw" + TRANSFORM(i) + ".value"
			IF &hf2  AND thisform.eq_adults.Value >= i then
				&hf = .t.
			ELSE
				&hf = .f.
			ENDIF
		NEXT i
		RETURN .t.
	ENDPROC


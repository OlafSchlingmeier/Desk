	PROCEDURE formeditbanquet.tbendtime.Valid
		l_Hours = Max(0, Min(23, Val(Substr(THIS.VALUE, 1, 2))))
		l_Minutes = Max(0, Min(59, Val(Substr(THIS.VALUE, 4, 2))))
		If !Empty(Substr(THIS.Value, 1, 2)+Substr(THIS.Value, 4, 2))
			THIS.VALUE = Padl(Ltrim(Str(l_Hours)), 2, "0")+":"+ PADl(Ltrim(Str(l_Minutes)), 2, "0")
		Else
			THIS.Value = "  :  "
		ENDIF
		IF !(thisformset.p_BanquetData.bq_endtime>=thisformset.p_BanquetData.bq_time .OR. thisformset.p_BanquetData.bq_endtime= '  :  ')
		*	WAIT WINDOW "End time must be greater than time"
			RETURN .F.
		endif
		
	ENDPROC


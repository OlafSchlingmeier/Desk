	PROCEDURE changemode
		LPARAMETERS nmode
		LOCAL LFactor
		
		LFactor = this.wsheet.cmdclose.Width / 24
		DO CASE
			CASE nmode=1
			*sheet
				this.wsheet.cmdcopy.Visible=.F.
				this.wsheet.cmdEdit.Visible=.F.
				this.wsheet.cmddel.l=24
				this.wsheet.cmddel.Left=24 * LFactor
				this.wsheet.cmdback.l=48
				this.wsheet.cmdback.Left=48 * LFactor
				this.wsheet.cmdback.Visible=.t.
				this.wsheet.cmdnext.l=72
				this.wsheet.cmdnext.Left=72 * LFactor
				this.wsheet.cmdnext.Visible=.t.
				this.wsheet.cmdclose.l=96
				this.wsheet.cmdclose.Left=96 * LFactor
				this.wsheet.tbday.Visible=.t.
				this.wsheet.lday.Visible=.t.
			CASE nmode=2
			*banquet
				this.wsheet.cmdback.Visible=.F.
				this.wsheet.cmdnext.Visible=.F.
				this.wsheet.cmdcopy.Visible=.t.
				this.wsheet.cmdedit.Visible=.t.
				this.wsheet.cmddel.l=72
				this.wsheet.cmddel.Left=72 * LFactor
				this.wsheet.cmdclose.L=96
				this.wsheet.cmdclose.Left=96 * LFactor
				this.wsheet.lday.Visible=.f.
				this.wsheet.tbday.Visible=.f.
		ENDCASE
		
		RETURN
		
	ENDPROC

	PROCEDURE Init
		LPARAMETERS pl_oRef
		this.AddProperty("p_callformref", NULL)
		this.p_CallFormRef = pl_oRef
		this.p_CallFormRef.Enabled = .F.
		this.p_readonly = (this.p_CallFormRef.Parent.mode = "HISTORY")
		this.p_BanquetFilter = FILTER('banquet')
		this.p_BanquetOrder = ORDER('banquet')
		
		IF this.p_readonly
			this.wsheet.cmdNew.Enabled = .F.
			this.wsheet.cmdcopy.Enabled = .F.
			this.wsheet.cmdEdit.Enabled = .F.
			this.wsheet.cmddel.Enabled = .F.
		ENDIF
		
		IF NOT (EMPTY(this.mRemoveTimeCharacterFromString(RESERVAT.RS_ARRTIME)) .OR. ;
				EMPTY(this.mRemoveTimeCharacterFromString(RESERVAT.RS_DEPTIME)))
			THISFORMSET.WSheet.PAGeFrame1.PAge1.Enabled=.t.
			THISFORMSET.WSheet.PAgeFrame1.PAge2.Enabled=.t.
			THISFORMSET.WSheet.PAgeFrame1.PAGe3.Enabled=.t.
			IF this.p_readonly
				this.mdisablecontrols()
			ENDIF
		ELSE
			this.wSheet.PageFrame1.ActivePage = 4
		ENDIF
		
		this.wsheet.pageframe1.page4.gdbanquet.RecordSource = [banquet]
		this.wsheet.pageframe1.page4.gdbanquet.cartinum.ControlSource = [banquet.bq_artinum]
		this.wsheet.pageframe1.page4.gdbanquet.CDATE.ControlSource = [banquet.bq_date]
		this.wsheet.pageframe1.page4.gdbanquet.cdesc.ControlSource = [banquet.bq_descrip]
		this.wsheet.pageframe1.page4.gdbanquet.cendtime.ControlSource = [banquet.bq_endtime]
		this.wsheet.pageframe1.page4.gdbanquet.cmemo.ControlSource = [SubStr(MLine(banquet.bq_memo, 1), 1, 30)]
		this.wsheet.pageframe1.page4.gdbanquet.cprice.ControlSource = [banquet.bq_price]
		this.wsheet.pageframe1.page4.gdbanquet.ctime.ControlSource = [banquet.bq_time]
		this.wsheet.pageframe1.page4.gdbanquet.cunits.ControlSource = [banquet.bq_units]
		this.wsheet.pageframe1.page4.gdbanquet.cwhere.ControlSource = [LTrim(banquet.bq_roomnum + ' ' + banquet.bq_where)]
		this.wsheet.pageframe1.page4.gdbanquet.cwho.ControlSource = [banquet.bq_who]
		SET ORDER TO TAG3 IN banquet
		SET FILTER TO banquet.bq_reserid = reservat.rs_reserid IN banquet
		GO TOP IN banquet
		
		SET ORDER TO TAG2 IN sheet
		SET FILTER TO IN sheet
		Thisformset.formSHEETDAY.tbday.Value=0
		
		this.refreshcurrentdate()
		this.WSheet.txtDate.ControlSource = [thisformset.dCurrentDate]
		
		
		this.AddProperty("p_BanquetData")

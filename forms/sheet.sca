	PROCEDURE mdelete
		DO CASE
		CASE this.wsheet.pageFrame1.ActivePage=4
			IF EMPTY(banquet.bq_reserid)
				RETURN
			ENDIF
			If 6=Messagebox(GetLangText("BANQUET","TA_DELETE")+CHR(13)+Str(banquet.bq_artinum)+" "+ Trim(banquet.bq_descrip)+" ?",36,GetLangText("FUNC","TXT_QUESTION"))
				DELETE IN banquet
				SKIP IN banquet
			Endif
			this.wsheet.pageframe1.page4.gdbanquet.SetFocus
		CASE INLIST(this.wsheet.pageFrame1.ActivePage,1,2,3)
			IF !EMPTY(sheet.sh_reserid)
				IF !sheet.sh_reserid<>reservat.rs_reserid
					If 6=Messagebox(GetLangText("SCREENS","TA_DELETE")+"?",36,GetLangText("FUNC","TXT_MESSAGE"))
						Delete Next 1 IN sheet
						SKIP 1 IN sheet
						IF sheet.sh_reserid<>reservat.rs_reserid .or. EOF('sheet')
							SKIP -1 IN sheet
							IF sheet.sh_reserid<>reservat.rs_reserid .or. BOF('sheet')
								GO BOTTOM in sheet
								SKIP 1 IN sheet
				*				this.mnew
							endif
						ENDIF
						thisformset.refreshcurrentdate()
					ENDIF
				ENDIF
			ENDIF
		ENDCASE
		
		this.wsheet.Refresh
		
	ENDPROC

	PROCEDURE mdisablecontrols		&& Disable all controls if mode of form is read-only.
		LOCAL i, j
		
		WITH this.wSheet.PageFrame1
			FOR j = 1 TO this.wSheet.PageFrame1.PageCount
				IF .Pages(j) <> .Page4
					FOR i = 1 TO .Pages(j).ControlCount
						IF .Pages(j).Controls(i).BaseClass = "Combobox"
							.Pages(j).Controls(i).Style = 0
						ENDIF
						IF .Pages(j).Controls(i).BaseClass <> "Label"
							.Pages(j).Controls(i).DisabledForeColor = 0
							.Pages(j).Controls(i).Enabled = .F.
						ENDIF
					ENDFOR
				ENDIF
			ENDFOR
		ENDWITH
	ENDPROC


	PROCEDURE wSheet.PageFrame1.Page5.cntConsumption.Init
		LOCAL l_nConsumption, l_cFilter, l_nArtinumLine1, l_nArtinumLine2, l_nArtinumLine3
		
		**** Fill article number for WATER, ELECTRIC POWER, GAS ***********************************
		l_nArtinumLine1 = 3100      && article.ar_artinum for WATER                               *
		l_nArtinumLine2 = 3200      && article.ar_artinum for ELECTRIC POWER                      *
		l_nArtinumLine3 = 3300      && article.ar_artinum for GAS                                 *
		*******************************************************************************************
		
		this.cmdSave.Enabled = (TYPE("sheet.sh_strval1") <> "U")
		IF this.cmdSave.Enabled
			SELECT sheet
			l_cFilter = FILTER()
			SET FILTER TO
			LOCATE FOR sh_reserid = reservat.rs_reserid AND sh_sheet = "VERBRAUC"
			IF NOT FOUND()
				INSERT INTO sheet (sh_reserid, sh_sheet) VALUES (reservat.rs_reserid, "VERBRAUC")
			ENDIF
			this.cntConsumption1.sh_strval.Value = sheet.sh_strval1
			this.cntConsumption1.sh_endval.Value = sheet.sh_endval1
			this.cntConsumption2.sh_strval.Value = sheet.sh_strval2
			this.cntConsumption2.sh_endval.Value = sheet.sh_endval2
			this.cntConsumption3.sh_strval.Value = sheet.sh_strval3
			this.cntConsumption3.sh_endval.Value = sheet.sh_endval3
			SET FILTER TO &l_cFilter
		ELSE
			MESSAGEBOX("If you want to save consumption data in SHEET.DBF you must update (h)sheet.dbfs structure!!!"+CHR(13)+;
					"(Add fields: sh_strval1 I, sh_endval1 I, sh_strval2 I, sh_endval2 I, sh_strval3 I, sh_endval3 I)",;
					48,"Desk - Consumption module")
		ENDIF
		
		***** Initialize 1st line - Water consumption *********
		WITH this.cntConsumption1
			.nArtinum = l_nArtinumLine1
			l_nConsumption = MAX(0, .sh_endval.Value - .sh_strval.Value)
			SELECT article
			LOCATE FOR ar_artinum = .nArtinum
			.nPrice = ar_price
			.lblArticle.Caption = ALLTRIM(EVALUATE("ar_lang"+g_langnum))
			.lblConsumption.Caption = TRANSFORM(l_nConsumption)
			.lblPrice.Caption = TRANSFORM(.nPrice)
			.lblAmount.Caption = TRANSFORM(l_nConsumption*.nPrice,"999,999,999.99 ")
		ENDWITH
		
		***** Initialize 2nd line - Power consumption *********
		WITH this.cntConsumption2
			.nArtinum = l_nArtinumLine2
			l_nConsumption = MAX(0, .sh_endval.Value - .sh_strval.Value)
			SELECT article
			LOCATE FOR ar_artinum = .nArtinum
			.nPrice = ar_price
			.lblArticle.Caption = ALLTRIM(EVALUATE("ar_lang"+g_langnum))
			.lblConsumption.Caption = TRANSFORM(l_nConsumption)
			.lblPrice.Caption = TRANSFORM(.nPrice)
			.lblAmount.Caption = TRANSFORM(l_nConsumption*.nPrice,"999,999,999.99 ")
		ENDWITH
		
		***** Initialize 3rd line - Gas consumption *********
		WITH this.cntConsumption3
			.nArtinum = l_nArtinumLine3
			l_nConsumption = MAX(0, .sh_endval.Value - .sh_strval.Value)
			SELECT article
			LOCATE FOR ar_artinum = .nArtinum
			.nPrice = ar_price
			.lblArticle.Caption = ALLTRIM(EVALUATE("ar_lang"+g_langnum))
			.lblConsumption.Caption = TRANSFORM(l_nConsumption)
			.lblPrice.Caption = TRANSFORM(.nPrice)
			.lblAmount.Caption = TRANSFORM(l_nConsumption*.nPrice,"999,999,999.99 ")
		ENDWITH
	ENDPROC


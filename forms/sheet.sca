	PROCEDURE mnew
		DO CASE
			CASE this.wsheet.pageFrame1.ActivePage=4
				this.wsheet.Enabled= .F.
				this.formeditbanquet.p_AppendNewRecordToBanquet = .T.
				SELECT banquet
				SCATTER MEMO NAME this.p_BanquetData BLANK
		
				this.p_BanquetData.bq_reserid = reservat.rs_reserid
				this.p_BanquetData.bq_date = reservat.rs_arrdate
				this.p_BanquetData.bq_time = reservat.rs_arrtime
		
				this.FORmeditbanquet.TBRscdesc.Value=Iif(Seek(Padr("RESOURCE",10)+Padr(banquet.bq_rsrc,3),"BQPICKLIST","TAG4"),Evaluate("BQPICKLIST.PL_LANG"+G_LANGNUM),"")
				this.FORmeditbanquet.TBWHo.Value=Iif(Seek(Padr("DEPARTMENT",10)+Padr(banquet.bq_who,3),"BQPICKLIST1","TAG4"),Evaluate("BQPICKLIST1.PL_LANG"+G_LANGNUM),"")
				this.FORmeditbanquet.TBMenu.Value=Iif(Seek(Padr("MENU",10)+Padr(banquet.bq_menu,3),"BQPICKLIST2","TAG4"),Evaluate("BQPICKLIST2.PL_LANG"+G_LANGNUM),"")
		
				this.FORmeditbanquet.Refresh()
				this.FORmeditbanquet.Show()
			CASE INLIST(this.wsheet.pageFrame1.ActivePage,1,2,3)
				IF reservat.rs_arrdate=reservat.rs_depdate
			* just show record
					IF NOT SEEK(STR(reservat.rs_reserid,12,3)+STR(0,2),"sheet",'tag2')
			* new record
						m.sh_day=0
						m.sh_reserid=reservat.rs_reserid
						DO CASE
							CASE this.wsheet.pageFrame1.ActivePage=1
								m.sh_sheet='TAGUNG'
							CASE this.wsheet.pageFrame1.ActivePage=2
								m.sh_sheet='BANKETT'
							CASE this.wsheet.pageFrame1.ActivePage=3
								m.sh_sheet='SHEET'
						ENDCASE
						INSERT INTO Sheet FROM MEMVAR
					ELSE
			* record already exist, show it
						alert(GetLangText("COMMON","TXT_RECORDEXISTS"))
					ENDIF
					this.WSheet.PAgeFrame1.Pages(this.wsheet.pageFrame1.ActivePage).ACTIVATE
					this.wsheet.Refresh
				ELSE
			* ask for day
					this.wsheet.Enabled=.F.
					this.forMSHEETDAY.Visible=.T.
					this.forMSHEETDAY.ZOrder(0)
				ENDIF
				thisformset.refreshcurrentdate()
		ENDCASE
		
	ENDPROC

	PROCEDURE mnext

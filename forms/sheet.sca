	PROCEDURE wSheet.KeyPress
		LPARAMETERS nKeyCode, nShiftAltCtrl
		
		DO case
			CASE nKeyCode=27 AND nShiftAltCtrl = 0 && Esc
				this.cmdclose.Click
				NODEFAULT	
			CASE nKeyCode=14 AND nShiftAltCtrl = 2 AND this.cmdnew.enabled&& New
				this.cmdnew.click
				NODEFAULT
			CASE nKeyCode=5 AND nShiftAltCtrl = 2 AND this.cmdedit.enabled&& Edit
				this.cmdedit.click
				NODEFAULT
			CASE nKeyCode=11 AND nShiftAltCtrl = 2 AND this.cmdcopy.enabled&& Copy
				this.cmdcopy.click
				NODEFAULT
			CASE nKeyCode=4 AND nShiftAltCtrl = 2 AND this.cmddel.enabled&& Delete
				this.cmddel.click
				NODEFAULT
			CASE nKeyCode=145 AND nShiftAltCtrl = 2 AND this.cmdnext.enabled&& Next
				this.cmdnext.click
				NODEFAULT
			CASE nKeyCode=141 AND nShiftAltCtrl = 2 AND this.cmdback.enabled&& Previous
				this.cmdback.click
				NODEFAULT
		ENDCASE
		
	ENDPROC


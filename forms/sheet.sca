	PROCEDURE mcheckresource		&& Function copied from banquet.prg
		LOCAL ncApacity, cdEscription, crEsource, ddAte, cbEgin, ceNd, nuSed
		
		ncApacity = BQpicklist.pl_numval
		cdEscription = Evaluate("BQPicklist.pl_lang"+g_langnum)
		crEsource = baNquet.bq_rsrc
		ddAte = baNquet.bq_date
		cbEgin = baNquet.bq_time
		ceNd = baNquet.bq_endtime
		nuSed = 0
		Scan All For baNquet.bq_date=ddAte .And. baNquet.bq_rsrc==crEsource  ;
				.And. baNquet.bq_time<=ceNd .And. baNquet.bq_endtime>=cbEgin
			nuSed = nuSed+baNquet.bq_rsrcqty
		Endscan
		If nuSed>ncApacity
			lcmsg=GetLangText("BANQUET","TA_RSCCAPACITY")
			lcmsg=STRTRAN(lcmsg,'%s1',ALLTRIM(cdEscription))
			lcmsg=STRTRAN(lcmsg,'%s2',ALLTRIM(Str(ncApacity)))
			lcmsg=STRTRAN(lcmsg,'%s3',ALLTRIM(Str(nuSed)))
			= Messagebox(lcmsg,48,GetLangText("FUNC","TXT_MESSAGE"))
		Endif
		
		RETURN
		
	ENDPROC


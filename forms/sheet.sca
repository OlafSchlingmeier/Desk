	PROCEDURE wSheet.PageFrame1.Page5.cntConsumption.cmdSave.Click
		LOCAL l_nArea, l_cFilter
		ASSERT .f.
		l_nArea = SELECT()
		
		SELECT sheet
		l_cFilter = FILTER()
		SET FILTER TO
		
		IF NOT (sh_reserid = reservat.rs_reserid AND sh_sheet = "VERBRAUC")
			LOCATE FOR sh_reserid = reservat.rs_reserid AND sh_sheet = "VERBRAUC"
		ENDIF
		
		REPLACE sh_strval1 WITH this.Parent.cntConsumption1.sh_strval.Value, ;
			   sh_endval1 WITH this.Parent.cntConsumption1.sh_endval.Value, ;
			   sh_strval2 WITH this.Parent.cntConsumption2.sh_strval.Value, ;
			   sh_endval2 WITH this.Parent.cntConsumption2.sh_endval.Value, ;
			   sh_strval3 WITH this.Parent.cntConsumption3.sh_strval.Value, ;
			   sh_endval3 WITH this.Parent.cntConsumption3.sh_endval.Value
		SET FILTER TO &l_cFilter
		
		SELECT (l_nArea)
	ENDPROC

	PROCEDURE wSheet.PageFrame1.Page5.cntConsumption.cntConsumption1.cmdBook.Click
		LOCAL l_nConsumption
		
		WITH this.Parent
			WAIT "Posting " + .lblArticle.Caption + " consumption..." WINDOW TIMEOUT 1
		
			IF NOT EMPTY(.nArtinum)
				l_nConsumption = MAX(0, .sh_endval.Value - .sh_strval.Value)
				PostArt(reservat.rs_reserid, 1, .nArtinum, l_nConsumption, .nPrice, "", .lblArticle.Caption + " consumption")
			ENDIF
		
			WAIT CLEAR
		ENDWITH
	ENDPROC


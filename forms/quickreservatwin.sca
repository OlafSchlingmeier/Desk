DEFINE CLASS frmqreswin AS tform OF "..\libs\main.vcx" 
 	*< CLASSDATA: Baseclass="form" Timestamp="" Scale="" Uniqueid="" />

	*-- OBJECTDATA items order determines ZOrder / El orden de los items OBJECTDATA determina el ZOrder 
	*< OBJECTDATA: ObjPath="opgQResAvail" UniqueID="" Timestamp="" />
	*< OBJECTDATA: ObjPath="cmdOk" UniqueID="" Timestamp="" />
	*< OBJECTDATA: ObjPath="cmdCancel" UniqueID="" Timestamp="" />

	*<DefinedPropArrayMethod>
		*p: nretvalue
	*</DefinedPropArrayMethod>

	Caption = "frmQResWin"
	ControlBox = .T.
	DataSession = 1
	DoCreate = .T.
	Height = 84
	KeyPreview = .T.
	MaxButton = .F.
	MinButton = .F.
	Name = "frmQResWin"
	nretvalue = 1
	Width = 311
	WindowType = 1

	ADD OBJECT 'cmdCancel' AS tcommandbutton WITH ;
		Caption = "cmdCancel", ;
		Height = 24, ;
		Left = 216, ;
		Name = "cmdCancel", ;

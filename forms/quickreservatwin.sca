		*< END OBJECT: ClassLib="..\libs\main.vcx" BaseClass="commandbutton" />

	ADD OBJECT 'cmdOk' AS tcommandbutton WITH ;
		Caption = "cmdOk", ;
		Height = 24, ;
		Left = 216, ;
		Name = "cmdOk", ;
		Top = 12
		*< END OBJECT: ClassLib="..\libs\main.vcx" BaseClass="commandbutton" />

	ADD OBJECT 'opgQResAvail' AS toptiongroup WITH ;
		ButtonCount = 2, ;
		Height = 46, ;
		Left = 12, ;
		Name = "opgQResAvail", ;
		Top = 12, ;

		LPARAM lcName
		LOCAL lii,loObj
		
		FOR lii=1 TO This.ControlCount
		    loObj=This.Controls(lii)
		    IF UPPER(loObj.BaseClass)="CHECKBOX" AND !loObj.Name==lcName
		       loObj.Value=.F.

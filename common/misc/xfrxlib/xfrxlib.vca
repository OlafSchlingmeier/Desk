		 This.cboZoom.AddItem(loCNT.GetString(__xfrxlib_Zoom_FWd)) AND ;
		 This.cboZoom.AddItem(loCNT.GetString(__xfrxlib_Zoom_FWi))
		
		*This.Caption=loCNT.GetString(__xfrxlib_tlbToolbar)
		

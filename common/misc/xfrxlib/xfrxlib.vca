		This.Parent.MouseMove(nButton, nShift, nXCoord, nYCoord,This)
		This.Parent.SetActiveObject(This,SUBSTR(PROGRAM(),RAT(".",PROGRAM())+1))
	ENDPROC

	PROCEDURE MouseUp

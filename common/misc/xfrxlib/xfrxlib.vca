	*< OBJECTDATA: ObjPath="cboVal" UniqueID="" Timestamp="" />

	#INCLUDE "xfrxlib.h"
	Height = 21
	Name = "xfrxal"
	Width = 260

	ADD OBJECT 'cboVal' AS xfrxcbo WITH ;
		BoundColumn = 2, ;
		ColumnCount = 2, ;
		ColumnWidths = "100,0", ;
		Left = 0, ;
		Name = "cboVal", ;
		Style = 2, ;
		Top = 0, ;
		Width = 100
		*< END OBJECT: ClassLib="xfrxlib.vcx" BaseClass="combobox" />
	
	PROCEDURE changevalue
		LPARAM luValue

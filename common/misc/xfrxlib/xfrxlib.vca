		thisform.oprinteroptions.alloddeven = thisform.xfrxcbo1.ListItemId
		thisform.oprinteroptions.copies = thisform.txtcopies.Value
		thisform.oprinteroptions.zoom = thisform.xfrxcbo2.ListItemId
		
		thisform.Release()
		
	ENDPROC


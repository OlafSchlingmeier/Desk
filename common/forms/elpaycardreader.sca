	PROCEDURE ocom.OnComm
		*** ActiveX Control Event ***
		LOCAL lcRawData, lcBuffer
		
		lcRawData = ""
		DO WHILE .T.
			lcBuffer = STRCONV(this.Input,15)
			IF LEN(lcBuffer) = 0
				EXIT
			ENDIF
			lcRawData = lcRawData + lcBuffer
			Sleep(50)
			DOEVENTS
		ENDDO
		IF LEN(lcRawData) > 0
			this.Parent.MsgRawDataReceive(lcRawData)
		ENDIF
	ENDPROC


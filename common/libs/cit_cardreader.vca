	PROCEDURE cmdSaveAll.Click
		LOCAL lcKey, lnSector, lnRetry
		
		lcKey = PADR(STRTRAN(thisform.txtKeyValue.vSelectedValue," "),12,"0")
		    
		FOR lnSector = 0 To thisform.oCallingObj.nMaxSectors - 1
			FOR lnRetry = 1 TO 3
				IF thisform.SaveKey(lnSector, lcKey)
					EXIT
				ENDIF
			NEXT
			DOEVENTS
		NEXT
	ENDPROC

ENDDEFINE

